<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ContactApp.Views.AddContact">

    <ContentPage.ToolbarItems>
        <ToolbarItem Text="Save" Command="{Binding SaveContact}"></ToolbarItem>
    </ContentPage.ToolbarItems>

    <ContentPage.Content>
        <StackLayout>

            <Entry  Text="{Binding Itemnew.Name}" />
            <Entry  Text="{Binding Itemnew.CellNumber}" />

        </StackLayout>

    </ContentPage.Content>

</ContentPage>